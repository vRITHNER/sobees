  <log4net>
    <!-- create a  file appender which logs a formatted log message to log.txt -->
    <appender name="FileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="sobees-logfile.log" />
      <appendToFile value="true" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <maxSizeRollBackups value="5" />
      <rollingStyle value="Date" />
      <datePattern value="yyyyMMdd" />
      <staticLogFileName value="True" />
      <countDirection value="1" />
      <layout type="log4net.Layout.PatternLayout">
        <!--<conversionPattern value="%logger: %date [%thread] %-5level - %message %newline" />-->
        <!--<conversionPattern value="%d [%t] %-5p %c [%x] - %m%n" />-->
        <conversionPattern value="%d{MM/dd/yyyy HH:mm:ss}: %p %t  %c{1} - %m%n" />
      </layout>
    </appender>
    <!-- configure the the root logger to ALL log level, i.e. no log messages are filtered
         and use the FileAppender configured above to log messages -->
    <root>
      <level value="ALL" />
      <appender-ref ref="FileAppender" />
    </root>
  </log4net>
