<Glass:GlassWindow x:Class="Sobees.About.Views.About"
                   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                   xmlns:Views="clr-namespace:Sobees.About.Views"
                   xmlns:Glass="clr-namespace:Sobees.Glass"
                   ResizeMode="NoResize"
                   CenterInParentWindow="True"
                   Title="About sobees..."
                   Height="500"
                   Width="500"
                   FontFamily="{DynamicResource FontFamilyGeneric}">

    <Grid x:Name="grMain"
          Margin="0,0,0,0">
        <Views:UcAbout Margin="0,0,0,0" HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch" />
    </Grid>


</Glass:GlassWindow>
