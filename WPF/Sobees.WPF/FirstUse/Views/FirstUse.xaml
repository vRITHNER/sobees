<Glass:GlassWindow x:Class="Sobees.FirstUse.Views.FirstUse"
                   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                   xmlns:Views="clr-namespace:Sobees.FirstUse.Views"
                   xmlns:BLocalizeLib="clr-namespace:Sobees.Library.BLocalizeLib;assembly=Sobees.Library.BLocalizeLib"
                   xmlns:Glass="clr-namespace:Sobees.Glass"
                   ShowInTaskbar="False"
                   Title="{BLocalizeLib:LocText Assembly=Sobees.Configuration.BGlobals, Dict=Resources, Key=txtWelcome}"
                   CenterInParentWindow="True"
                   Height="530"
                   Width="550"
                   ResizeMode="NoResize"
                   FontFamily="{DynamicResource FontFamilyGeneric}">
  <Grid x:Name="grMain"
        Margin="0,0,0,0">
        <Views:UcFirstUse />
    </Grid>
</Glass:GlassWindow>
