    <Style x:Key="ContentControlStyleBg"
           TargetType="{x:Type ContentControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <Border CornerRadius="5"
                            Visibility="Visible">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="399,493.25"
                                                 StartPoint="399,0"
                                                 MappingMode="Absolute">
                                <GradientStop Color="#E4212121"
                                              Offset="0.069" />
                                <GradientStop Color="#F12B2B2B"
                                              Offset="1" />
                                <GradientStop Color="#001A1A1A" />
                            </LinearGradientBrush>
                        </Border.Background>
                        <ContentPresenter Visibility="Collapsed" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Margin"
                Value="1" />
    </Style>
    <Style x:Key="ContentControlStyleBgMultipost"
           TargetType="{x:Type ContentControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <Border CornerRadius="5"
                            Visibility="Visible"
                            BorderBrush="#FF636B76"
                            BorderThickness="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1"
                                                 StartPoint="0.5,0">
                                <GradientStop Color="#FF2F343A"
                                              Offset="0" />
                                <GradientStop Color="#FF434951"
                                              Offset="1" />
                            </LinearGradientBrush>
                        </Border.Background>
                        <ContentPresenter Visibility="Collapsed" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Margin"
                Value="1" />
    </Style>
