    <Style x:Key="LabelContentControlTitleLevel1"
           TargetType="{x:Type ContentControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <Grid d:DesignHeight="39.5"
                          d:DesignWidth="167.75">
                        <Border BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="2,2,2,2"
                                Background="{TemplateBinding Background}"
                                CornerRadius="1,1,1,1">
                            <Border BorderBrush="#2B000000"
                                    BorderThickness="0.5,0.5,0.5,0.5"
                                    Margin="1,1,1,1">
                                <Rectangle Stroke="{x:Null}"
                                           Height="7"
                                           VerticalAlignment="Bottom"
                                           RadiusX="1"
                                           RadiusY="10"
                                           Margin="-2,0,-2,-10">
                                    <Rectangle.OpacityMask>
                                        <LinearGradientBrush EndPoint="0.943,0.5"
                                                             StartPoint="0.062,0.5">
                                            <GradientStop Color="#E4000000"
                                                          Offset="0.077" />
                                            <GradientStop Color="#EA000000"
                                                          Offset="0.95" />
                                            <GradientStop Color="#9A7E7E7E"
                                                          Offset="0.357" />
                                            <GradientStop Color="#975C5C5C"
                                                          Offset="0.667" />
                                            <GradientStop Color="#5F000000"
                                                          Offset="0" />
                                            <GradientStop Color="#CB000000"
                                                          Offset="1" />
                                        </LinearGradientBrush>
                                    </Rectangle.OpacityMask>
                                    <Rectangle.Fill>
                                        <LinearGradientBrush EndPoint="0.5,1"
                                                             StartPoint="0.5,0">
                                            <GradientStop Color="#35000000"
                                                          Offset="0.183" />
                                            <GradientStop Color="#00000000"
                                                          Offset="1" />
                                            <GradientStop Color="#92000000"
                                                          Offset="0" />
                                        </LinearGradientBrush>
                                    </Rectangle.Fill>
                                </Rectangle>
                            </Border>
                        </Border>
                        <TextBlock HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                   VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                   Text="{TemplateBinding Content}"
                                   TextWrapping="Wrap"
                                   Margin="10,5"
                                   TextAlignment="Center" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Background"
                Value="White" />
        <Setter Property="BorderBrush"
                Value="#FFF8F8F8" />
        <Setter Property="Foreground"
                Value="#FF454545" />
        <Setter Property="FontFamily"
                Value="Fonts/#DINSKI CASUAL CONDENSED" />
        <Setter Property="FontSize"
                Value="17.333" />
        <Setter Property="MaxWidth"
                Value="190" />
        <Setter Property="RenderTransformOrigin"
                Value="0.5,0.5" />
        <Setter Property="RenderTransform">
            <Setter.Value>
                <TransformGroup>
                    <ScaleTransform />
                    <SkewTransform />
                    <RotateTransform Angle="-2" />
                    <TranslateTransform />
                </TransformGroup>
            </Setter.Value>
        </Setter>
        <Setter Property="FontWeight"
                Value="Bold" />
        <Setter Property="HorizontalAlignment"
                Value="Center" />
        <Setter Property="Margin"
                Value="0,0,0,20" />
    </Style>

