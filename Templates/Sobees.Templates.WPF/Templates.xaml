    <Style x:Key="ContentControlStyleAvatar"
           TargetType="{x:Type ContentControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <Grid>
                        <Rectangle Stroke="{x:Null}"
                                   Height="6"
                                   Margin="3,49,3,0"
                                   VerticalAlignment="Top"
                                   RadiusX="1"
                                   RadiusY="10"
                                   Grid.Row="0"
                                   Grid.RowSpan="1"
                                   OpacityMask="{DynamicResource ShadowAvatarOpacityMask}"
                                   Fill="{DynamicResource ShadowAvatarFill}"
                                   Visibility="Visible" />
                        <Border HorizontalAlignment="Left"
                                Margin="2,2,0,0"
                                Grid.Column="0"
                                VerticalAlignment="Top"
                                Grid.RowSpan="1"
                                d:LayoutOverrides="GridBox"
                                Width="47"
                                Height="47"
                                Background="{DynamicResource Level2ItemBg}"
                                OpacityMask="{DynamicResource dbMaskAvatar}">
                            <ContentPresenter Width="47"
                                              Height="47"
                                              Content="{TemplateBinding Content}"
                                              HorizontalAlignment="Stretch"
                                              Margin="0" />
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Focusable"
                Value="False" />
        <Setter Property="MinWidth"
                Value="0" />
        <Setter Property="MinHeight"
                Value="0" />
        <Setter Property="HorizontalAlignment"
                Value="Left" />
        <Setter Property="VerticalAlignment"
                Value="Top" />
        <Setter Property="Margin"
                Value="0,0,5,0" />
    </Style>
